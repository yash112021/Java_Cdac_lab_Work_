package com.demo.beans;

import javax.persistence.Entity;
import javax.persistence.FetchType;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import javax.persistence.OneToOne;
import javax.persistence.Table;

@Entity
@Table(name="jy_add")
public class Address {
	@Id
	@GeneratedValue
private int aid;
private String city;
private int pincode;
@OneToOne(mappedBy= "yash", fetch= FetchType.LAZY) //mappedBy will not add foreign key
private User usr;

public Address() {
	super();
}
public Address(int aid, String city, int pincode, User usr) {
	super();
	this.aid = aid;
	this.city = city;
	this.pincode = pincode;
	this.usr = usr;
}
public int getAid() {
	return aid;
}
public void setAid(int aid) {
	this.aid = aid;
}
public String getCity() {
	return city;
}
public void setCity(String city) {
	this.city = city;
}
public int getPincode() {
	return pincode;
}
public void setPincode(int pincode) {
	this.pincode = pincode;
}
public User getUsr() {
	return usr;
}
public void setUsr(User usr) {
	this.usr = usr;
}
@Override
public String toString() {
	return "Address [aid=" + aid + ", city=" + city + ", pincode=" + pincode + ", usr=" + usr + "]";
}

}
