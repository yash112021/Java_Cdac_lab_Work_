package com.demo.beans;

public class Address {
	// this will have the id ,city and pinccode as the data member
	private int aid;
	private String city ;
	private String pincode;
	
	// default constructopr
	public Address() {
		super();
	}

	// paramterised constructor 
	public Address(int aid, String city, String pincode) {
		super();
		this.aid = aid;
		this.city = city;
		this.pincode = pincode;
	}
	
	
	
	
	
}
