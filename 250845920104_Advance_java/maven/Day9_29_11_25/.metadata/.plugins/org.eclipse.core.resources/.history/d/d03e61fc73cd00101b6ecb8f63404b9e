package com.demo.beans;

public class User {
	// by using the annotations
	private int uid;
	private String unmae;
	private String email;
	
	
	// default constructor
	public User() {
		super();
	}

	// paramterised constructor
	public User(int uid, String unmae, String email) {
		super();
		this.uid = uid;
		this.unmae = unmae;
		this.email = email;
	}
	
	
	
	
	
}
