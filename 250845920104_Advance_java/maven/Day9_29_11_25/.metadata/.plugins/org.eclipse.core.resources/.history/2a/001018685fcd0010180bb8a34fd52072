<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<!-- now here we need to the Mapping of the  User files -->
<!--if you are not using the annotations then you have to create this files  -->

<hibernate-mapping>
	<class name="com.demo.beans.User">
	<id name="empno">
		<generator class="assigned"></generator>
	</id>
	
	private int empno;
	private String Ename;
	private String job;
	private String usrname;
	private String pwd;
	private String role; 
	<!--EmpNo, Ename , job ,user,name ,pwd ,role-->
	<property name="ename" type="String" column="EmpNo" not-null="true"></property>
	<property name="job" type="String" column="job"></property>
	<property name="usrname" type="String" column="usern">
</hibernate-mapping>
