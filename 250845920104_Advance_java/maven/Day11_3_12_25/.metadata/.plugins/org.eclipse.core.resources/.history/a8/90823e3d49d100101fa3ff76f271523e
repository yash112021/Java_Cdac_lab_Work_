package com.demo.beans;

import java.time.LocalDate;

public class NonPerishable extends Product {
	// child class which is having the gurantee
	private int gurantee;

	//default constuctor 
	public NonPerishable() {
		super();
	}

	// parmaterised constructor 
	public NonPerishable(int pid, String pname, int qty, double price, LocalDate mfgDate, int gurantee) {
		super(pid, pname, qty, price, mfgDate);
		this.gurantee = gurantee;
	}


	// To getter and method
	public int getGurantee() {
		return gurantee;
	}


	public void setGurantee(int gurantee) {
		this.gurantee = gurantee;
	}

	@Override
	public String toString() {
		return super.toString()+"NonPerishable [gurantee=" + gurantee + "]";
	}
	
	// To String 
	
	
		
}
