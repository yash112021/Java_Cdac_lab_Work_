package com.demo.beans;

import java.time.LocalDate;

public class Perishable extends Product {
	// child class of the product which is haveing the data Member expriy date
	private LocalDate ldt;

	// default constructor
	public Perishable() {
		super();
	}

	// paramterised constructor 
	public Perishable(int pid, String pname, int qty, double price, LocalDate mfgDate, LocalDate ldt) {
		super(pid, pname, qty, price, mfgDate);
		this.ldt = ldt;
	}
	
	
	
	
}
