package com.demo.beans;

import java.time.LocalDate;
import java.util.Set;

import org.hibernate.annotations.Entity;

@Entity
@Table(name="Emp1")
public class Emplyee {
	// data members id,name,date sal and set of project
	private int eid;
	private String name;
	private LocalDate date;
	private double sal;
	// one data members is set of the project 
	Set<Project>eset;
	
	// default construciotr 
	public Emplyee() {
		super();
	}

	// parmatersied constructor 
	public Emplyee(int eid, String name, LocalDate date, double sal, Set<Project> eset) {
		super();
		this.eid = eid;
		this.name = name;
		this.date = date;
		this.sal = sal;
		this.eset = eset;
	}

	// getters and Setters
	public int getEid() {
		return eid;
	}

	public void setEid(int eid) {
		this.eid = eid;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public LocalDate getDate() {
		return date;
	}

	public void setDate(LocalDate date) {
		this.date = date;
	}

	public double getSal() {
		return sal;
	}

	public void setSal(double sal) {
		this.sal = sal;
	}

	public Set<Project> getEset() {
		return eset;
	}

	public void setEset(Set<Project> eset) {
		this.eset = eset;
	}

	// ToString Method
	@Override
	public String toString() {
		return "Emplyee [eid=" + eid + ", name=" + name + ", date=" + date + ", sal=" + sal + "]";
	}
	
	
	
	
	
	
}
	