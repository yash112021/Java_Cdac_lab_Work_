package com.demo.bean;

public class Emlpyee {
	// data Memebers 
	private int eid;
	private String ename;
	private double sal;
	
	
	// default constrtor
	public Emlpyee() {
		super();
	}

	// paramterised constructor 
	public Emlpyee(int eid, String ename, double sal) {
		super();
		this.eid = eid;
		this.ename = ename;
		this.sal = sal;
	}
	
	
	
}
