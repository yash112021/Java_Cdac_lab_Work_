package com.demo.bean;

public class MyUser {
	// data Members id,name,email,address
	private  int uid;
	private String uname;
	private String uemail;
	private Address addr;
	
	// default constructor 
	public MyUser() {
		super();
	}

	// parmaterised constructor 
	public MyUser(int uid, String uname, String uemail, Address addr) {
		super();
		this.uid = uid;
		this.uname = uname;
		this.uemail = uemail;
		this.addr = addr;
	}

	
	// getters and setters
	public int getUid() {
		return uid;
	}

	public void setUid(int uid) {
		this.uid = uid;
	}

	public String getUname() {
		return uname;
	}

	public void setUname(String uname) {
		this.uname = uname;
	}

	public String getUemail() {
		return uemail;
	}

	public void setUemail(String uemail) {
		this.uemail = uemail;
	}

	public Address getAddr() {
		return addr;
	}

	public void setAddr(Address addr) {
		this.addr = addr;
	}
	
	// ToSTring Method 
	
	
	
	
}
