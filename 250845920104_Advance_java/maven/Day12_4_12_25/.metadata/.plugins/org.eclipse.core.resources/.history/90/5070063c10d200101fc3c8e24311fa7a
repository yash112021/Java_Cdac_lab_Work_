package com.demo.bean;

public class Emlpyee {
	// data Memebers 
	private int eid;
	private String ename;
	private double sal;
	
	
	// default constrtor
	public Emlpyee() {
		super();
	}

	// paramterised constructor 
	public Emlpyee(int eid, String ename, double sal) {
		super();
		this.eid = eid;
		this.ename = ename;
		this.sal = sal;
	}

	// getters and setters
	public int getEid() {
		return eid;
	}

	public void setEid(int eid) {
		this.eid = eid;
	}

	public String getEname() {
		return ename;
	}

	public void setEname(String ename) {
		this.ename = ename;
	}

	public double getSal() {
		return sal;
	}

	public void setSal(double sal) {
		this.sal = sal;
	}

	
	
	
}
