package com.demo.bean;

public class Skill {
	// data Members 
	private int sid;
	private String skname;
	private int experi;
	
	//default constructor 
	public Skill() {
		super();
	}

	// parmaterised constrcutor 
	public Skill(int sid, String skname, int experi) {
		super();
		this.sid = sid;
		this.skname = skname;
		this.experi = experi;
	}
	
	
	
	
}
