package com.demo.bean;

public class Address {
	// data Members 
	private String street;
	private String city;
	private String pincode;
	
	
	
	// paramterised constructor 
	public Address(String street, String city, String pincode) {
		super();
		this.street = street;
		this.city = city;
		this.pincode = pincode;
	}
	
	
	
}
