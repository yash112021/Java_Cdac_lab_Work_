package com.demo.beans;

public class User {
	private String name;
	private String password;
	private String email;
	
	// defalut constructor
	public User() {
		super();
	}

	// paramterised constructor
	public User(String name, String password, String email) {
		super();
		this.name = name;
		this.password = password;
		this.email = email;
	}
	
}
